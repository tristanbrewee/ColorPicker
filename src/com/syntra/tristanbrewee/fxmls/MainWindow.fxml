         <center>
            <Button fx:id="btnStart" mnemonicParsing="false" onAction="#btnStartClicked" text="Start" textAlignment="CENTER" textFill="#f80707" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Fira Code Regular" size="24.0" />
               </font>
            </Button>
         </center>
      </BorderPane>
   </children>
