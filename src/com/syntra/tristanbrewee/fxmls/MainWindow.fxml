<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.syntra.tristanbrewee.controllers.MainWindowController">
   <children>
      <BorderPane layoutX="116.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
         <top>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="TOP_CENTER">
               <children>
                  <Text fill="#ff1a00" strokeType="OUTSIDE" strokeWidth="0.0" text="Color Picker Game" textAlignment="CENTER">
                     <font>
                        <Font name="Fira Code Regular" size="36.0" />
                     </font>
                  </Text>
               </children>
            </StackPane>
